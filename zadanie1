package academy.learnprogramming;

public class Main {

    public static void main(String[] args) {
        String input  = "ATGCTGCATTCTTAGCAAG";
        input = TtoU(input);
        System.out.println(input);
    }

    public static String TtoU (String DNA) {
        String RNA = DNA.replace('T' , 'U');
        return RNA;
    }
}
